<% layout('layouts/boilerplate') %>




    <!-- End Side Vav -->
    <a class="close_side_menu" href="javascript:void(0);"></a>
    <div class="rbt-page-banner-wrapper  background-choice-gradient-vertical">
        <!-- Start Banner BG Image  -->
        <div class="rbt-banner-image"></div>
        <!-- End Banner BG Image  -->
    </div>
    <!-- Start Card Style -->
    <div class="rbt-dashboard-area rbt-section-overlayping-top rbt-section-gapBottom">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <!-- Start Dashboard Top  -->
                    <div class="rbt-dashboard-content-wrapper">
                        <div class="tutor-bg-photo bg_image bg_image--23 height-350">
                        </div>
                        <!-- Start Tutor Information  -->
                        <div class="rbt-tutor-information">
                            <div class="rbt-tutor-information-left">
                                <div class="thumbnail rbt-avatars size-lg">
                                    <img src="/img/female-profile.jpg" alt="Instructor">
                                </div>
                                <div class="tutor-content">
                                    <h5 class="title">
                                        <%= student.fullName %>
                                    </h5>
                                    <ul class="rbt-meta rbt-meta-white mt--5">
                                        <li> <img src="/svg/class.svg" width="15" alt="">&nbsp; Enrolled</li>
                                        <li><img src="/svg/certificate.svg" width="15" alt="">&nbsp;0 Certificate</li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                        <!-- End Tutor Information  -->
                    </div>
                    <!-- End Dashboard Top  -->

                    <div class="row g-5">
                        <div class="col-lg-3">
                            <!-- Start Dashboard Sidebar  -->
                            <div class="rbt-default-sidebar sticky-top rbt-shadow-box rbt-gradient-border">
                                <div class="inner">
                                    <div class="content-item-content">

                                        <div class="rbt-default-sidebar-wrapper">
                                            <div class="section-title mb--20">
                                                <h6 class="rbt-title-style-2">Welcome, Jone Due</h6>
                                            </div>
                                            <nav class="mainmenu-nav">
                                                <ul class="dashboard-mainmenu rbt-default-sidebar-list">
                                                    <li><a href="student-enrolled-courses.html"><img
                                                                src="/svg/class.svg" width="15" alt=""> &nbsp;
                                                            <span>Enrolled
                                                                Class</span></a></li>
                                                    <li><a href="student-my-quiz-attempts.html"><img src="/svg/exam.svg"
                                                                width="15" alt=""> &nbsp;<span>My assignment
                                                                Attempts</span></a></li>
                                                    </li>
                                                </ul>
                                            </nav>

                                            <div class="section-title mt--40 mb--20">
                                                <h6 class="rbt-title-style-2">User</h6>
                                            </div>

                                            <nav class="mainmenu-nav">
                                                <ul class="dashboard-mainmenu rbt-default-sidebar-list">
                                                    <li><a href="#"><img src="/svg/setting.svg" width="15" alt="">
                                                            &nbsp;<span>Settings</span></a>
                                                    </li>
                                                    <li><a href="/logout"><img src="/svg/logout.svg" width="15" alt="">
                                                            &nbsp;<span>Logout</span></a></li>
                                                </ul>
                                            </nav>
                                        </div>

                                    </div>
                                </div>
                            </div>
                            <!-- End Dashboard Sidebar  -->
                        </div>

                        <div class="col-lg-9">
                            <div class="rbt-dashboard-content bg-color-white rbt-shadow-box mb--60">
                                <div class="content">
                                    <div class="section-title">
                                        <h4 class="rbt-title-style-3">Dashboard</h4>
                                    </div>
                                    <div class="row g-5">

                                        <!-- Start Single Card  -->
                                        <div class="col-lg-4 col-md-4 col-sm-6 col-12">
                                            <div
                                                class="rbt-counterup variation-01 rbt-hover-03 rbt-border-dashed bg-primary-opacity">
                                                <div class="inner">
                                                    <div class="rbt-round-icon bg-primary-opacity">
                                                        <img width="80" src="/svg/class.svg" alt="enrolled Class">
                                                    </div>
                                                    <div class="content">
                                                        <h3 class="without-icon color-primary">
                                                            <span class="">Enrolled
                                                                Class</span>
                                                        </h3>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <!-- End Single Card  -->

                                        <!-- Start Single Card  -->
                                        <div class="col-lg-4 col-md-4 col-sm-6 col-12">
                                            <div
                                                class="rbt-counterup variation-01 rbt-hover-03 rbt-border-dashed bg-secondary-opacity">
                                                <div class="inner">
                                                    <div class="rbt-round-icon bg-secondary-opacity">
                                                        <img src="/svg/achievement.svg" alt="Current Grade">
                                                    </div>
                                                    <div class="content">
                                                        <h3 class="without-icon color-primary">
                                                            <span class="">Your Grade</span>
                                                        </h3>
                                                    </div>
                                                </div>
                                            </div>

                                        </div>
                                        <!-- End Single Card  -->

                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- End Card Style -->
    <div class="rbt-separator-mid">
        <div class="container">
            <hr class="rbt-separator m-0">
        </div>
    </div>

    <%- include('../partials/footer') %>


        </body>

        </html>