                                                                                    role="tablist">
                                                                                    <li class="nav-item tabs__tab "
                                                                                        role="presentation">
                                                                                        <button class="nav-link"
                                                                                            id="home-tab-5"
                                                                                            data-bs-toggle="tab"
                                                                                            data-bs-target="#home5<%= subject._id %>"
                                                                                            type="button" role="tab"
                                                                                            aria-controls="home5<%= subject._id %>"
                                                                                            aria-selected="false">Materials</button>
                                                                                    </li>
                                                                                    <li class="nav-item tabs__tab "
                                                                                        role="presentation">
                                                                                        <button class="nav-link"
                                                                                            id="profile-tab5"
                                                                                            data-bs-toggle="tab"
                                                                                            data-bs-target="#profile5<%= subject._id %>"
                                                                                            type="button" role="tab"
                                                                                            aria-controls="profile5<%= subject._id %>"
                                                                                            aria-selected="false">Assignments</button>
                                                                                    </li>
                                                                                </ul>
                                                                                <div
                                                                                    class="rainbow-tab-content tab-content p-0">
                                                                                    <div class="tab-pane fade"
                                                                                        id="home5<%= subject._id %>"
                                                                                        role="tabpanel"
                                                                                        aria-labelledby="home-tab-5">
