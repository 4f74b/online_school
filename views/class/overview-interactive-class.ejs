<% layout('layouts/boilerplate') %>

    <div class="rbt-page-banner-wrapper background-choice">
        <!-- Start Banner BG Image  -->
        <div class=""></div>
        <!-- End Banner BG Image  -->
    </div>

    <div class="rbt-section-overlayping-top rbt-section-gapBottom">
        <div class="inner">
            <div class="container">
                <div class="col-lg-12">

                    <div class="row row--30 gy-5 pt--60">


                        <div class="col-lg-4">
                            <div class="course-sidebar sticky-top rbt-shadow-box rbt-gradient-border">
                                <div class="inner">
                                    <div class="content-item-content">
                                        <div class="rbt-widget-details has-show-more">
                                            <div class="section-title">
                                                <h4 class="rbt-title-style-3">Class Overview</h4>
                                            </div>
                                            <ul class="has-show-more-inner-content rbt-course-details-list-wrapper">
                                                <li><span>Start Date</span><span class="rbt-feature-value rbt-badge-5">
                                                        <%= `${cls.created.toLocaleString('default', { month: 'short'
                                                            })} ${cls.created.getDate()} ${cls.created.getFullYear()}`;
                                                            %>
                                                    </span>
                                                </li>
                                                <% if(cls.students.length){ %>
                                                    <li><span>Enrolled</span><span
                                                            class="rbt-feature-value rbt-badge-5">
                                                            <%= cls.students.length %>
                                                        </span></li>
                                                    <% } %>
                                                        <% if(cls.courses.length){ %>
                                                            <li><span>Subjects</span><span
                                                                    class="rbt-feature-value rbt-badge-5">
                                                                    <%= cls.courses.length %>
                                                                </span></li>
                                                            <% } %>

                                                                <li><span>Class Level</span><span
                                                                        class="rbt-feature-value rbt-badge-5">
                                                                        <%= cls.classLevel %>
                                                                    </span>
                                                                </li>
                                                                <li><span>Certificate</span><span
                                                                        class="rbt-feature-value rbt-badge-5">Yes</span>
                                                                </li>
                                                                <li><span>Pass Percentage</span><span
                                                                        class="rbt-feature-value rbt-badge-5">95%</span>
                                                                </li>
                                            </ul>
                                            <div class="rbt-show-more-btn">Show More</div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="col-lg-8">
                            <!-- Start Course Details  -->
                            <div class="course-details-content">
                                <div class="rbt-inner-onepage-navigation sticky-top">
                                    <nav class="mainmenu-nav onepagenav">
                                        <ul class="mainmenu">
                                            <li class="">
                                                <a href="#coursecontent">Subject</a>
                                            </li>
                                            <li>
                                                <a href="#review">Review</a>
                                            </li>
                                        </ul>
                                    </nav>
                                </div>



                                <!-- Start Course Content  -->
                                <div class="rbt-instructor rbt-shadow-box intructor-wrapper mt--30 p-0"
                                    id="coursecontent">

                                    <div class="course-content rbt-shadow-box coursecontent-wrapper mt--30  p-1 p-sm-2 p-lg-4"
                                        id="coursecontent">

                                        <div class="rbt-course-feature-inner">
                                            <div class="section-title">
                                                <h4 class="rbt-title-style-3">Class Subjects</h4>
                                            </div>
                                            <% if (cls.courses.length){ %>
                                                <div class="rbt-accordion-style rbt-accordion-02 accordion"
                                                    style="margin-left: 2rem;">
                                                    <div class="accordion" id="accordionExampleb2">
                                                        <% for (let subject of cls.courses){ %>

                                                            <div class="accordion-item card">
                                                                <h2 class="accordion-header card-header"
                                                                    id="headingTwo2">
                                                                    <div class="h4 p-0 m-0" style="cursor: pointer;"
