                                                                        data-bs-target="#collapseTwo2<%= subject._id %>"
                                                                        aria-expanded="false"
                                                                        aria-controls="collapseTwo2<%= subject._id %>">
                                                                        <div class="d-flex justify-content-between">
                                                                            <div
                                                                                class=" d-flex justify-content-left align-items-center">
                                                                                <%= subject.name.charAt(0).toUpperCase()+
                                                                                    subject.name.slice(1) %> <span
                                                                                        class="rbt-badge-5 ml--10">2hr
                                                                                        30min</span>
                                                                            </div>
                                                                            <svg width="20" fill="#000000"
                                                                                viewBox="0 0 32 32" version="1.1"
                                                                                xmlns="http://www.w3.org/2000/svg"
                                                                                transform="rotate(180)">
                                                                                <g id="SVGRepo_bgCarrier"
                                                                                    stroke-width="0"></g>
                                                                                <g id="SVGRepo_tracerCarrier"
                                                                                    stroke-linecap="round"
                                                                                    stroke-linejoin="round"></g>
                                                                                <g id="SVGRepo_iconCarrier">
                                                                                    <path
                                                                                        d="M0.256 23.481c0 0.269 0.106 0.544 0.313 0.75 0.412 0.413 1.087 0.413 1.5 0l14.119-14.119 13.913 13.912c0.413 0.413 1.087 0.413 1.5 0s0.413-1.087 0-1.5l-14.663-14.669c-0.413-0.412-1.088-0.412-1.5 0l-14.869 14.869c-0.213 0.212-0.313 0.481-0.313 0.756z">
                                                                                    </path>
                                                                                </g>
                                                                            </svg>

                                                                        </div>

                                                                    </div>
                                                                </h2>

                                                                <div id="collapseTwo2<%= subject._id %>"
                                                                    class="accordion-collapse collapse"
                                                                    aria-labelledby="headingTwo2"
                                                                    data-bs-parent="#accordionExampleb2">
                                                                    <div class="accordion-body card-body pr--0">
                                                                        <div class="rbt-author-meta mb--10">
                                                                            <div class="rbt-avater">
                                                                                <a
                                                                                    href="<%= `/teacher/${subject.teacher._id}/view` %>">
                                                                                    <img src="/img/female-profile.jpg"
                                                                                        alt="Sophia Jaymes">
                                                                                </a>
                                                                            </div>
                                                                            <div class="rbt-author-info">
                                                                                By <a
                                                                                    href="<%= `/teacher/${subject.teacher._id}/view` %>">
                                                                                    <%= subject.teacher.userInfo.fullName
                                                                                        %>
                                                                                </a>
                                                                            </div>
                                                                        </div>

                                                                        <div class="col-12">
                                                                            <div class="rbt-default-tab">
