async function getTeacher() {
document.querySelector('.coursesSelect').onchange = async function (evt) {
document.getElementById('selectedLabelTeacher').innerHTML = this.value;
document.getElementById('timeLabelWrap').classList.add('d-none');
if (this.value && !document.querySelector('input[name="slot"]:checked')) {
getTeacherByCourse()
} else if (this.value) {
getTeacherByCourseAndSlot()
}
}
}